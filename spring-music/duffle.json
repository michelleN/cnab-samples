{
    "name": "spring-music",
    "version": "0.1.0",
    "description": "A short description of your bundle",
    "maintainers": [
        {
            "name": "Brian Redmond",
            "email": "john.doe@example.com",
            "url": "https://example.com"
        },
        {
            "name": "Jane Doe",
            "email": "jane.doe@example.com",
            "url": "https://example.com"
        }
    ],
    "invocationImages": {
        "spring-app-installer": {
            "name": "spring-app-installer",
            "builder": "docker",
            "configuration": {
                "registry": "chzbrgr71"
            }
        }
    },
    "parameters": {
      "cluster_name": {
        "defaultValue": "briar-aks-ml-200",
        "type": "string",
        "destination": {
          "env": "CLUSTER_NAME"
        }
      },
      "aks_resource_group": {
        "defaultValue": "briar-aks-ml-200",
        "type": "string",
        "destination": {
          "env": "AKS_RESOURCE_GROUP"
        }
      },
      "app_resource_group": {
        "defaultValue": "cnab-bundle-demo",
        "type": "string",
        "destination": {
          "env": "APP_RESOURCE_GROUP"
        }
      },
      "cosmosdb_service_name": {
        "defaultValue": "briarmusic200",
        "type": "string",
        "destination": {
          "env": "COSMOSDB_SERVICE_NAME"
        }
      },
      "cosmosdb_location": {
        "defaultValue": "eastus",
        "type": "string",
        "destination": {
          "env": "COSMOSDB_LOCATION"
        }
      }
    },
    "credentials": {
      "CLIENT_ID":
        {"env":"AZURE_CLIENT_ID"},
      "CLIENT_SECRET":
        {"env":"AZURE_CLIENT_SECRET"},
      "SUBSCRIPTION_ID":
        {"env":"AZURE_SUBSCRIPTION_ID"},
      "TENANT_ID":
        {"env":"AZURE_TENANT_ID"}
    }
}
